xstar = [6 -2; -1 8]\[1 0]';
F = @(x) [6*x(1) - cos(x(1)) - 2*x(2); 8*x(2) - x(1)*x(2)^2 - sin(x(1))];
DF = @(x) [6+sin(x(1)), -2; -x(2)^2-cos(x(1)), 8-2*x(1)*x(2)];
x0 = [0;0];
tol = 10^(-10);

[x,xk] = newton(F,DF,x0,tol,30)

err=[];
for i=1:size(xk,2)
  err(i) = norm(xstar-xk(:,i));
end
Error = [[1:size(xk,2)].', err.']
format short

